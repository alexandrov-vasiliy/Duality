      \u0447\u0442\u043E \u0431\u0443\u0434\u0435\u0442 \u0434\u0430\u043B\u044C\u0448\u0435\u2026"
    mercyText: "\u041D\u0438\u043A\u0442\u043E \u043D\u0435 \u0443\u0437\u043D\u0430\u043B
      \u043E \u0442\u043E\u043C \u0447\u0442\u043E \u0441\u043B\u0443\u0447\u0438\u043B\u043E\u0441\u044C,
